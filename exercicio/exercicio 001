#include <stdio.h>

int main() {
    int numeroDeVisitantes;
    printf("Digite o número de visitantes de hoje: ");
    scanf("%d", &numeroDeVisitantes);
    printf("Número de visitantes registrado: %d\n", numeroDeVisitantes);
    printf("Obrigado! O registro foi feito com sucesso.\n");
    return 0;
}
