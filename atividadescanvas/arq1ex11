#include <stdio.h>
#include <math.h>

int main() {
    int n10, n50, n100, r100, r50, r10, valor;
    printf ("informe o valor recebido: ");
    scanf ("%d", &valor);
    n100 = valor / 100;
    r100 = valor % 100;
    n50 = r100 / 50;
    r50 = r100 % 50;
    n10 = r50 / 10;
    r10 = r50 % 10;
    printf ("vao ser necessarias:\n %d notas de 100\n %d notas de 50\n %d notas de 10\n %d notas de 1", n100, n50, n10, r10);
    return (0);
}
